{
	"name": "@ank1015/providers",
	"version": "0.0.37",
	"type": "module",
	"description": "Multi-provider LLM SDK with native provider options support.",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"README.md"
	],
	"scripts": {
		"build": "tsc",
		"dev": "tsc --watch",
		"start": "node dist/index.js",
		"clean": "rm -rf dist",
		"check": "biome check --write . && tsc --noEmit",
		"test": "vitest --run",
		"test:unit": "vitest --run tests/unit",
		"prepublishOnly": "npm run check && npm run test:unit && npm run build"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/ank1015/providers.git"
	},
	"keywords": [
		"ai",
		"llm",
		"agent",
		"openai",
		"gemini",
		"providers",
		"sdk",
		"autonomous"
	],
	"author": "ank1015",
	"license": "MIT",
	"bugs": {
		"url": "https://github.com/ank1015/providers/issues"
	},
	"homepage": "https://github.com/ank1015/providers#readme",
	"dependencies": {
		"@anthropic-ai/sdk": "^0.71.0",
		"@biomejs/biome": "^2.3.8",
		"@google/genai": "^1.30.0",
		"@sinclair/typebox": "^0.34.41",
		"ajv": "^8.17.1",
		"ajv-formats": "^3.0.1",
		"openai": "^6.9.1",
		"partial-json": "^0.1.7",
		"uuid": "^13.0.0"
	},
	"devDependencies": {
		"@mendable/firecrawl-js": "^4.9.3",
		"@types/node": "^24.10.1",
		"@types/uuid": "^11.0.0",
		"chalk": "^5.6.2",
		"diff": "^8.0.2",
		"file-type": "^21.1.1",
		"glob": "^13.0.0",
		"parallel-web": "^0.2.4",
		"tsx": "^4.21.0",
		"typescript": "^5.9.3",
		"vitest": "^4.0.14"
	}
}
